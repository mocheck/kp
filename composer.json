{
    "name": "mocheck/kp",
    "description": "Laravel wrapper for Kinopoisk API",
    "keywords": [
        "laravel",
        "kinopoisk",
        "api"
    ],
    "license": "proprietary",
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.0",
        "illuminate/support": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Wielski\\Kinopoisk\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wielski\\Kinopoisk\\Providers\\KinopoiskServiceProvider"
            ],
            "aliases": {
                "Kinopoisk": "Wielski\\Kinopoisk\\Facades\\Kinopoisk"
            }
        }
    }
}
